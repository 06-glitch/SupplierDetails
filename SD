                    "    d.Vendor_Id " &_
                    "  , d.Vendor_Name " &_
                    "  , d.Main_Contact " &_
                    "  , d.Contact_Phone " &_
                    "  , d.Contact_Email " &_
                    "  , d.Quality_Contact " &_
                    "  , d.Quality_Phone " &_
                    "  , d.Quality_Email " &_
                    "  , d.eSMI_Flag " &_
                    "  , d.Auto_Email " &_
                    "  , d.Auto_Email_Recipient " &_
                    "  , d.New_PO_Email " &_
                    "  , d.Mid_LT_Email " &_
                    "  , d.Approaching_Due_Email " &_
                    "  , d.Past_Due_Email " &_
                    "FROM ct_Supplier_Details d (NOLOCK) " &_
                    "WHERE d.Vendor_Id LIKE '%" & SearchTerms & "%' " 

