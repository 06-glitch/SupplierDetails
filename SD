                      "    c.Vendor_Id " &_
                      "  , c.Contact_Name " &_
                      "  , c.Office_Phone " &_
                      "  , c.Office_Phone_Ext " &_
                      "  , c.Mobile_Nbr " &_
                      "  , c.Fax_Nbr " &_
                      "  , c.Email_Addr " &_
