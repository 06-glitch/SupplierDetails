                        "    im.MMM_Id_Nbr " &_
                        "  , im.Item_Desc " &_
                        "  , im.Family_Code " &_
                        "  , Analyst = h.Title_Column2 " &_
                        "  , Engineer = COALESCE(ec.Eng_Full_Name, im.c_spfd_Eng_Code) " &_
                        "FROM t_Item_Master im (NOLOCK)  " &_
